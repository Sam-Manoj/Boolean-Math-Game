<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>
<StackPane fx:id="stackPane" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javaminiproject.AboutController">
    <ImageView fx:id="backgroundImageView" pickOnBounds="true" preserveRatio="true ">
        <image>
            <Image url="@ab_gm.png"/>
        </image>
    </ImageView>

    <Button fx:id="helloButton" onAction="#BackClick" alignment="CENTER_RIGHT" contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false" prefHeight="138.0" prefWidth="413.0" translateX="535.0">
        <graphic>
            <ImageView>
                <image>
                    <Image url="@/com/example/javaminiproject/Back.png"/>
                </image>
            </ImageView>
        </graphic>
    </Button>
</StackPane>
