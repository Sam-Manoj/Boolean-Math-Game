<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>



<StackPane fx:id="stackPane" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javaminiproject.HighscoreController">
    <ImageView fx:id="backgroundImageView" pickOnBounds="true" preserveRatio="true">
        <image>
            <Image url="@Highscore_bg.png" />
        </image>
    </ImageView>

    <Label fx:id="highScoreUser1" layoutX="20" layoutY="20" style="-fx-font-size: 48px; -fx-font-weight: bold;-font-family: 'Comic Sans MS';" textFill="yellow" translateY="-84.0" />
    <Label fx:id="highScoreUser2" layoutX="20" layoutY="60" style="-fx-font-size: 48px; -fx-font-weight: bold;-font-family: 'Comic Sans MS';" textFill="deeppink" translateX="286.0" translateY="80.0" />
    <Label fx:id="highScoreUser3" layoutX="20" layoutY="100" style="-fx-font-size: 48px; -fx-font-weight: bold;-font-family: 'Comic Sans MS';" textFill="orangered" translateX="-291.0" translateY="166.0" />

    <Button fx:id="helloButton" alignment="CENTER_RIGHT" contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false" onAction="#BackClick" prefHeight="138.0" prefWidth="413.0" translateX="543.0" translateY="-226.0">
        <graphic>
            <ImageView>
                <image>
                    <Image url="@/com/example/javaminiproject/Back.png" />
                </image>
            </ImageView>
        </graphic>
    </Button>
</StackPane>
